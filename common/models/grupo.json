{
  "name": "Grupo",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "tutor": {
      "type": "string"
    },
    "anyo": {
      "type": "string",
      "required": true
    },
    "grupo": {
      "type": "string",
      "required": true
    },
    "subgrupo": {
      "type": "string",
      "required": true,
      "default": "ensenanza"
    },
    "ensenanza": {
      "type": "string"
    },
    "curso": {
      "type": "number"
    },
    "horarioVisita": {
      "type": "string"
    },
    "archivos": {
      "type": "array",
      "default": []
    }
  },
  "validations": [],
  "relations": {
    "matriculas": {
      "type": "hasMany",
      "model": "Matricula",
      "foreignKey": "id"
    },
    "materiaimpartidaids": {
      "type": "hasMany",
      "model": "MateriaImpartida",
      "foreignKey": "id"
    },
    "profesores": {
      "type": "belongsTo",
      "model": "Profesor",
      "foreignKey": "id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
